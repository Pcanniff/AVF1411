var appCloudLogin = function() {

	if (Ti.Network.online) {
		Cloud.Users.login({
			login : "com.fullsail.demoapp",
			password : "12345"
		}, function(e) {
			if (e.success) {
				Ti.API.info("Logged in user, id = " + users[0].id + ", session ID = " + Cloud.sessionId);
			} else {
				Ti.API.info("Login failed.");
			}
		});
	}
};


exports.appCloudLogin = appCloudLogin;
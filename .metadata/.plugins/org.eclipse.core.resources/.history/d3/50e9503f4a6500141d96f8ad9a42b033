var cloudSave = function(obj) {
	Cloud.Objects.create({
		classname : 'weather',
		fields : {
			tempF : obj.tempF,
			tempC : obj.tempC,
			city: obj.city,
			state: obj.state,
			wind: obj.wind
		}
	}, function(e) {
		if (e.success) {
			var car = e.cars[0];
			alert('Success:\n' + 'id: ' + car.id + '\n' + 'make: ' + car.make + '\n' + 'color: ' + car.color + '\n' + 'year: ' + car.year + '\n' + 'created_at: ' + car.created_at);
		} else {
			alert('Error:\n' + ((e.error && e.message) || JSON.stringify(e)));
		}
	});
}; 